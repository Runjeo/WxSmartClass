<cu-custom bgColor="bg-gradual-blue" isBack="{{true}}">
    <view slot="content">拍摄照片</view>
</cu-custom>
<camera binderror="error" devicePosition="front" flash="off" style="width: 100%; height: 300px"></camera>
<view class="flex">
    <view class="flex-sub    margin-xs radius">
        <button bindtap="start" class="cu-btn bg-{{backgroud}} margin-tb-sm lg">{{text}}</button>
    </view>
    <view class="flex-sub   margin-xs radius">
        <button bindtap="goback" class="cu-btn bg-{{backgroud1}} margin-tb-sm lg" disabled="{{disable}}">确定使用</button>
    </view>
</view>
<view>预览</view>
<image src="{{src}}" style="width: 50%; height: 150px"></image>
